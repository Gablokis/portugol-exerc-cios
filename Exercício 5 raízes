programa {
  funcao inicio() {
    real a 
    real b
    real c
    real delta
    real raiz1
    real raiz2


    escreva("Digite o valor de a: ")
    leia(a)
    escreva("Digite o valor de b: ")
    leia(b)
    escreva("Digite o valor de c: ")
    leia(c)

    delta = b * b - 4 * a * c

    raiz1 = (-b + delta) / (2 * a)
    raiz2 = (-b - delta) / (2 * a)

    escreva("As raízes da equação são: ", raiz1, " e ", raiz2)
  }
}
