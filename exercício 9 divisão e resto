programa {
  funcao inicio() {
    inteiro num1
    inteiro num2
    inteiro resto

    escreva("Digite o primeiro número inteiro: ")
    leia(num1)
    escreva("Digite o segundo número inteiro: ")
    leia(num2)

    resto = num1 % num2


    escreva("O resto da divisão inteira de ", num1, " por ", num2, " é: ", resto)

  }
}
