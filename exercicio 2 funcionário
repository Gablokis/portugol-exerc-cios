programa {
  funcao inicio() {
   caracter  nomeFuncionario
   inteiro quantidadeDependentes
   caracter cargoFuncionario

    escreva("Digite o nome do funcion치rio: ")
    leia(nomeFuncionario)

    escreva("Digite a quantidade de dependentes: ")
    leia(quantidadeDependentes)

    escreva("Digite o cargo do funcion치rio: ")
    leia(cargoFuncionario)

    escreva("Nome do funcion치rio: ", nomeFuncionario, "\n")
    escreva("Quantidade de dependentes: ", quantidadeDependentes, "\n")
    escreva("Cargo do funcion치rio: ", cargoFuncionario, "\n")
  }
}
